}
layers {
  name: "final_fc"
  type: INNER_PRODUCT
  bottom: "hzvt6"
  top: "final_fc"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  inner_product_param {
    num_output: 400
    weight_filler {
      type: "msra"
      variance_norm: AVERAGE
    }
    bias_filler {
      type: "constant"
      value: 0
