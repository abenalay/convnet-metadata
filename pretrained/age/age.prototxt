  bottom: "final_fc"
  top: "final_fc_mod"
}
layers {
  name: "drop_final_fc"
  type: DROPOUT
  bottom: "final_fc_mod"
  top: "final_fc_mod"
  dropout_param {
    dropout_ratio: 0.44
  }
}
layers {
  name: "final_class"
  type: INNER_PRODUCT
  bottom: "final_fc_mod"
  top: "final_class"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  inner_product_param {
    num_output: 3
    weight_filler {
      type: "xavier"
      variance_norm: AVERAGE
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layers {
  name: "pred"
  type: SOFTMAX
  bottom: "final_class"
  top: "pred"
}
layers {
  name: "loss"
  type: SOFTMAX_LOSS
  bottom: "final_class"
  bottom: "label"
  top: "loss"
}