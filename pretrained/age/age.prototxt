  convolution_param {
    num_output: 58
    kernel_w: 4
    kernel_h: 1
    stride_w: 1
    stride_h: 1
    weight_filler {
      type: "msra"
      variance_norm: AVERAGE
    }
    bias_filler {
      type: "constant"
    }
  }
}
layers {
  name: "relu_hzvt3"
