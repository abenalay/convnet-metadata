  type: CONVOLUTION
  bottom: "hzvt4"
  top: "hzvt5"
  blobs_lr: 1
  blobs_lr: 2
  convolution_param {
    num_output: 130
    kernel_w: 12
    kernel_h: 1
    stride_w: 1
    stride_h: 1
    weight_filler {
      type: "msra"
      variance_norm: AVERAGE
    }
    bias_filler {
      type: "constant"
