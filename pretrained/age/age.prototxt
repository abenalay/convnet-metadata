name: "CDR-CNN"
layers {
  name: "data"
  type: HDF5_DATA
  top: "data"
  top: "label"
  hdf5_data_param {
    source: "/home/bjarke/caffe/trydeeplearning/convmeta/convmeta/deep/data/train.txt"
    batch_size: 256
    shuffle: false
  }
}
layers {
  name: "hzvt1"
  type: CONVOLUTION
  bottom: "data"
  top: "hzvt1"
  blobs_lr: 1
  blobs_lr: 2
  convolution_param {
    num_output: 26
    kernel_w: 4
    kernel_h: 1
    stride_w: 1
    stride_h: 1
    weight_filler {
      type: "msra"
      variance_norm: AVERAGE
    }
    bias_filler {
      type: "constant"
    }
  }
}
